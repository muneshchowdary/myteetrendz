{
  "family": "myteetrends-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::872111493182:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "myteetrends-container",
      "image": "872111493182.dkr.ecr.ap-south-1.amazonaws.com/myteetrends:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
